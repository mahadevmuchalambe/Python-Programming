
grocery_items = [
    ("Milk", 2.50),
    ("Bread", 1.80),
    ("Eggs", 3.20),
    ("Rice", 4.00),
    ("Apples", 2.75)
]
print("Grocery Items and Prices:")
total_price = 0
for item in grocery_items:
    name, price = item
    print(f"{name}: ${price:.2f}")
    total_price += price
print(f"\nTotal Price: ${total_price:.2f}")
